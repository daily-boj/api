{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Daily BOJ Member Schema",
    "description": "The object represents the member of daily-boj",
    "default": {},
    "examples": [
        {
            "id": "ranolp",
            "display_name": "Ranol☆P",
            "bio": "Lazy, but get Better.",
            "gravatar_email": "public.ranolp@gmail.com",
            "baekjoon_id": "asdhugh1",
            "webpage_url": "https://twitter.com/RanolP_777",
            "github_id": "RanolP",
            "codeforces_id": "RanolP",
            "codeup_id": "asdhugh1"
        }
    ],
    "required": [
        "id",
        "display_name",
        "baekjoon_id"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "string",
            "title": "Id",
            "description": "The unique id of the member.",
            "examples": [
                "ranolp"
            ]
        },
        "display_name": {
            "$id": "#/properties/display_name",
            "type": "string",
            "title": "Display Name",
            "description": "The display name of the member.",
            "examples": [
                "Ranol☆P"
            ]
        },
        "bio": {
            "$id": "#/properties/bio",
            "type": "string",
            "title": "Bio",
            "description": "The bio of the member.",
            "examples": [
                "Lazy, but get Better."
            ]
        },
        "profile_image_url": {
            "$id": "#/properties/profile_image_url",
            "type": "string",
            "title": "Profile Image URL",
            "description": "The profile image URL of the member.",
            "examples": [
                "https://gravatar.com/avatar/8ef181fc6a8aaefd5e285af21a159c52.png?s=512"
            ]
        },
        "gravatar_email": {
            "$id": "#/properties/gravatar_email",
            "type": "string",
            "title": "Gravatar Email",
            "description": "The Gravatar email, that is being for profile image, of the member.",
            "examples": [
                "public.ranolp@gmail.com"
            ]
        },
        "baekjoon_id": {
            "$id": "#/properties/baekjoon_id",
            "type": "string",
            "title": "Baekjoon ID",
            "description": "The Baekjoon Online Judge ID of the member.",
            "examples": [
                "asdhugh1"
            ]
        },
        "webpage_url": {
            "$id": "#/properties/webpage_url",
            "type": "string",
            "title": "Web Page URL",
            "description": "A Web page URL that belongs to the member.",
            "examples": [
                "https://twitter.com/RanolP_777"
            ]
        },
        "github_id": {
            "$id": "#/properties/github_id",
            "type": "string",
            "title": "GitHub ID",
            "description": "A GitHub ID of the member.",
            "examples": [
                "RanolP"
            ]
        },
        "codeforces_id": {
            "$id": "#/properties/codeforces_id",
            "type": "string",
            "title": "CodeForces ID",
            "description": "A CodeForces ID of the member.",
            "examples": [
                "RanolP"
            ]
        },
        "codeup_id": {
            "$id": "#/properties/codeup_id",
            "type": "string",
            "title": "CodeUp ID",
            "description": "A CodeUp ID of the member.",
            "examples": [
                "asdhugh1"
            ]
        }
    }
}